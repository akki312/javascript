<!-- views/movies/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Movie Listings</title>
</head>
<body>
    <h1>Movie Listings</h1>
    <ul>
        <% movies.forEach(movie => { %>
            <li><%= movie.title %></li>
        <% }); %>
    </ul>
</body>
</html>
